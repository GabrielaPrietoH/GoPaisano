<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/calendarioEmpresa"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Presentation.calendarioEmpresa">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/textView10"
            android:layout_width="179dp"
            android:layout_height="152dp"
            android:layout_above="@+id/calendarCliente"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="53dp"
            android:layout_marginEnd="11dp"
            android:layout_marginBottom="79dp"
            android:text="Calendario Citas Empresa"
            android:textAlignment="center"
            android:textColor="#2196F3"
            android:textSize="34sp" />

        <ImageView
            android:id="@+id/imageView10"
            android:layout_width="wrap_content"
            android:layout_height="164dp"
            android:layout_above="@+id/calendarCliente"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="12dp"
            android:layout_marginTop="43dp"
            android:layout_marginEnd="196dp"
            android:layout_marginBottom="43dp"
            android:src="@drawable/paisano" />

        <CalendarView
            android:id="@+id/calendarEmpresa"
            android:layout_width="368dp"
            android:layout_height="414dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="23dp"
            android:layout_marginEnd="20dp"
            android:layout_marginBottom="36dp" />
    </RelativeLayout>
</FrameLayout>